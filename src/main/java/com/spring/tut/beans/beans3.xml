<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">



	<bean id="jungle" class="com.spring.tut.test3.Jungle">
		<property name="largest" ref="elephant"></property>
		<property name="animals">
			<list>
				<ref bean="lion" />
				<ref bean="snake" />
			</list>
		</property>
	</bean>


	<bean id="lion" class="com.spring.tut.test3.Animal">
	
	<property name="name" value="Igor"></property>
	<property name="type" value="lion"></property>
	</bean>

	<bean id="elephant" class="com.spring.tut.test3.Animal">
	<property name="name" value="Richard"></property>
	<property name="type" value="elephant"></property>
	</bean>

	<bean id="snake" class="com.spring.tut.test3.Animal">
	<property name="name" value="Bob"></property>
	<property name="type" value="snake"></property>
	</bean>

</beans>
