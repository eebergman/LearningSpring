<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="robit" class="com.spring.tut.test5.Robit">
		<property name="id" value="123456"></property>
		<property name="speech" value="#{speechoutput.getText()}"></property>
	</bean>

	<bean id="speechoutput" class="com.spring.tut.test5.SpeechOutput"></bean>

</beans>
